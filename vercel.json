{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/transfer/(.*)",
      "methods": ["GET"],
      "dest": "/api/transfer/$1.js"
    },
    {
      "src": "/api/(.*)",
      "methods": ["GET"],
      "dest": "/api/$1.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 